{
  jwt_secret: 'demo123',
  acme_home: '~/.acme.sh/',
  urls: ['localhost','loractive.io','dev.rt0s.com','ari.rt0s.com','www.ztereoid.net'],
  users: [
    {
      username: 'anon',
      password: 'anon',
      acl: 'anon',
      rate: 10,
      max_sessions: 6
    },
    {
      username: 'demo',
      password: 'demo',
      acl: 'default',
      rate: 100,
      max_sessions: 6
    }
  ],
  ports: [
    {path: '/dev/tty.usbserial-FTD3SEN6x', type: 'hdlc', baudrate:9600}
  ],
  runners: {
    r_mongoDb: {bin: './runners/r_mongoDb.js', args: {}, run:true},
    duh555: {bin: './runners/duh.js', args: {id:555}},
    r_serial_hdlc: {bin: './runners/r_serial_hdlc.js', args: {schema: 'base_mqtt.json5'}},
    // duh333: {bin: './runners/duh.js', args: {id:333}},
  },
  sockets: [
    {
      name: 'http',
      port: 8090,
      protocol: 'http',
      sites: [
        {
          name: 'localhost',
          redir: 'https://loractive.io:9091/',
          urls: ['localhost','dev.rt0s.com']
        },
        {
          name: 'ari',
          static: 'web/localhost',
          urls: ['ari.rt0s.com','www.ztereoid.net']
        }
      ]
    },
    {
      name: 'https',
      port: 9091,
      protocol: 'https',
      sites: [
        {
          name: 'loractive.io',
          static: 'web/localhost',
          urls: ['loractive.io','dev.rt0s.com','www.ztereoid.net','ztereoid.net','dev.ztereoid.net']
        }
      ]
    },
    {
      name: 'mqtt',
      port: 8091,
      protocol: 'mqtt',
      max_connected: 200,
      url: 'localhost',
      users: ['*']
    },
    {
      name: 'ws',
      port: 8092,
      protocol: 'ws',
      max_connected: 200,
      url: 'localhost',
      users: ['demo', 'anon']
    }
  ],
}
